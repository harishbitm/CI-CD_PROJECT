pipeline{
agent any 
stages{
stage('mvn install'){
steps{
sh 'mvn clean install'
sh 'mv agent/target/*.jar agent/target/myapp.jar'
}
}
stage('deploy on tomcat-dev'){
steps{
sshagent(['dockerlog']) {
   sh 'scp -o StrictHostKeyChecking=no agent/target/myapp.jar ubuntu@54.91.51.146:/opt/tomcat8/webapps/'
}
}
}
}
}
