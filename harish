pipeline{
agent any 
stages{
stage('mvn install'){
steps{
sh 'mvn clean install'
sh 'mv agent/target/*.jar agent/target/myapp.jar'
}
}
stage('deploy on tomcat-dev'){
steps{
sshagent(['dockerlog']) {
   sh 'ssh -o StrictHostKeyChecking=no agent/target/myapp.jar ubuntu@52.205.128.242:opt/tomcat/webapps/'
}
}
}
}
}
